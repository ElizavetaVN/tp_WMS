@model Domain.Entities.RegistrationWrite

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>RegistrationWrite</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Status" /> @Html.DisplayNameFor(model => model.Status)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="RegistrationWriteType" class="control-label"></label>
                <p>

                    @Html.DropDownListFor(model => model.RegistrationWriteType, ViewBag.RegistrationWriteType as SelectList)
                </p>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
